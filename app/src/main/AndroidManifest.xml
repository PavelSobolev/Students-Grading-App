        </activity>
        <activity android:name=".PredmActivity" />
        <activity android:name=".ZanActivity" />
        <activity android:name=".OtmActivity" />
        <!-- android:theme="@style/Theme.AppCompat.Light.Dialog" -->
        <activity android:name=".ZadanieActivity" />
